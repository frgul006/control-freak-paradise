{
  "name": "@cfp/cli",
  "version": "0.0.0",
  "private": true,
  "main": "./src/index.ts",
  "bin": {
    "cfp": "./bin/index.js"
  },
  "scripts": {
    "build": "yarn tsc && yarn chmod",
    "chmod": "chmod u+x ./bin/index.js",
    "clean": "rm ./bin/*(.)",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "test": "jest --detectOpenHandles",
    "dev": "yarn tsc --watch",
    "start": "node -r esbuild-register \"./src/index.ts\" \"$@\""
  },
  "jest": {
    "preset": "@cfp/jest-presets/jest/node"
  },
  "dependencies": {
    "@cfp/logger": "*",
    "chalk": "^4",
    "inquirer": "^9.1.4",
    "yargs": "^17.7.1",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@cfp/eslint-config": "*",
    "@cfp/jest-presets": "*",
    "@cfp/tsconfig": "*",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "esbuild": "^0.17.0",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "typescript": "^4.5.3"
  }
}
